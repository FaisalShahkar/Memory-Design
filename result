P_0x5f66e565dca0 .param/l "DATA_WIDTH" 0 3 1, +C4<00000000000000000000000000100000>;
v0x5f66e565fc40_0 .net "clk", 0 0, v0x5f66e568b210_0;  1 drivers
v0x5f66e568ab10 .array "mem", 0 5, 31 0;
v0x5f66e568abd0_0 .net "rd_addr", 7 0, v0x5f66e568b2d0_0;  1 drivers
v0x5f66e568acc0_0 .var "rd_data", 31 0;
v0x5f66e568ada0_0 .net "rd_en", 0 0, v0x5f66e568b4a0_0;  1 drivers
v0x5f66e568aeb0_0 .net "wr_addr", 7 0, v0x5f66e568b570_0;  1 drivers
v0x5f66e568af90_0 .net "wr_data", 31 0, v0x5f66e568b660_0;  1 drivers
v0x5f66e568b070_0 .net "wr_en", 0 0, v0x5f66e568b730_0;  1 drivers
E_0x5f66e56779e0 .event posedge, v0x5f66e565fc40_0;
    .scope S_0x5f66e5638ff0;
T_0 ;
    %wait E_0x5f66e56779e0;
    %load/vec4 v0x5f66e568ada0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %ix/getv 4, v0x5f66e568abd0_0;
    %load/vec4a v0x5f66e568ab10, 4;
    %assign/vec4 v0x5f66e568acc0_0, 0;
T_0.0 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5f66e5638ff0;
T_1 ;
    %wait E_0x5f66e56779e0;
    %load/vec4 v0x5f66e568b070_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %load/vec4 v0x5f66e568af90_0;
    %ix/getv 3, v0x5f66e568aeb0_0;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x5f66e568ab10, 0, 4;
T_1.0 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5f66e5638d90;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x5f66e568b210_0;
    %inv;
    %store/vec4 v0x5f66e568b210_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x5f66e5638d90;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f66e568b210_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f66e568b730_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f66e568b4a0_0, 0, 1;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x5f66e568b570_0, 0, 8;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x5f66e568b2d0_0, 0, 8;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5f66e568b660_0, 0, 32;
    %end;
    .thread T_3;
    .scope S_0x5f66e5638d90;
T_4 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f66e568b730_0, 0, 1;
    %pushi/vec4 3, 0, 8;
    %store/vec4 v0x5f66e568b570_0, 0, 8;
    %pushi/vec4 10, 0, 32;
    %store/vec4 v0x5f66e568b660_0, 0, 32;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f66e568b730_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f66e568b4a0_0, 0, 1;
    %pushi/vec4 3, 0, 8;
    %store/vec4 v0x5f66e568b2d0_0, 0, 8;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
